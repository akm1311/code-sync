# Database Configuration
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Vercel Blob Storage (auto-provided by Vercel when deployed)
# You don't need to set this manually - Vercel sets it automatically
# BLOB_READ_WRITE_TOKEN=vercel_blob_rw_xxxxx

# Node Environment (set to 'production' in Vercel)
NODE_ENV=development

# Server Port (only used in local development)
PORT=5000

